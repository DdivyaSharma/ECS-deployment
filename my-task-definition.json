{
  "family": "ecs-nodejs-task",
  "requiresCompatibilities": ["FARGATE"],
  "containerDefinitions": [
    {
      "name": "ecs-node-js",
      "image": "471112985676.dkr.ecr.ap-south-1.amazonaws.com/ecs-node-js:latest",
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3031,
          "hostPort": 3031
        }
      ]
    }
  ],
  "cpu": "256",
  "memory": "512",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::471112985676:role/ecsTaskExecutionRole"
}
